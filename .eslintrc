{
  "env": {
    "node": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "no-console": "error"
  }
}
